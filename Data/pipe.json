{
    "reader": {
        "bounds": "([-10425171.940, -10423171.940], [5164494.710, 5166494.710])",
        "filename": "https://s3-us-west-2.amazonaws.com/usgs-lidar-public/IA_FullState/ept.json",
        "type": "readers.ept",
        "tag": "readdata"
    },
    "cropping_filter": {
        "polygon": "POLYGON((0 0, 5000 10000, 10000 0, 0 0))",
        "type": "filters.crop"
    },
    "sampling_filter": {
        "type": "filters.sample",
        "radius": 1
    },
    "range_filter": {
        "limits": "Classification![7:7]",
        "type": "filters.range",
        "tag": "nonoise"
    },
    "assign_filter": {
        "assignment": "Classification[:]=0",
        "tag": "wipeclasses",
        "type": "filters.assign"
    },
    "reprojection_filter": {
        "in_srs": "EPSG:3857",
        "out_srs": "EPSG:26915",
        "tag": "reprojectUTM",
        "type": "filters.reprojection"
    },
    "smr_filter": {
        "tag": "groundify",
        "type": "filters.smrf"
    },
    "smr_range_filter": {
        "limits": "Classification[2:2]",
        "type": "filters.range",
        "tag": "classify"
    },
    "laz_writer": {
        "filename": "iowa.laz",
        "inputs": [
            "classify"
        ],
        "tag": "writerslas",
        "type": "writers.las"
    },
    "tif_writer": {
        "filename": "iowa.tif",
        "gdalopts": "tiled=yes,compress=deflate",
        "inputs": [
            "writerslas"
        ],
        "nodata": -9999,
        "output_type": "idw",
        "resolution": 1,
        "type": "writers.gdal",
        "window_size": 6
    }
}
